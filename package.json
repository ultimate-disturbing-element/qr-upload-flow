{
  "name": "qr-upload-flow",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./styles.css": {
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist",
    "src/styles.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "qr",
    "upload",
    "flow",
    "photo",
    "capture",
    "sync"
  ],
  "author": "Abhinav Rai",
  "license": "MIT",
  "description": "A lightweight SDK for QR-based photo capture and upload flows",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "qr-code-styling": "^1.9.2",
    "socket.io-client": "^4.8.1",
    "sortablejs": "^1.15.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
